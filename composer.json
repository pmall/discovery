{
    "name": "quanta/discovery",
    "type": "library",
    "description": "Library for collecting classes installed through composer",
    "keywords": [
        "composer",
        "discovery"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quantaphp/discovery",
    "support": {
        "issues": "https://github.com/quantaphp/discovery/issues",
        "source": "https://github.com/quantaphp/discovery"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^2.0",
        "kahlan/kahlan": "^4.0",
        "phpstan/phpstan": "^0.10.8"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\Discovery\\": "src"
        }
    }
}
